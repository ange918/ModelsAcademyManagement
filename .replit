modules = ["web", "nodejs-20", "python-3.12"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Web Server"

[[workflows.workflow]]
name = "Web Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 37901
externalPort = 3000

[[ports]]
localPort = 41517
externalPort = 4200

[[ports]]
localPort = 43353
externalPort = 3002

[[ports]]
localPort = 43561
externalPort = 3001

[[ports]]
localPort = 45069
externalPort = 3003

[deployment]
deploymentTarget = "autoscale"
run = ["python", "server.py"]
